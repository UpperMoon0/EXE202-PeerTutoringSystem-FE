{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch FE",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "preLaunchTask": "npm: dev",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    }
  ]
}
